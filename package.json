{
  "name": "subly",
  "version": "0.1.0",
  "homepage": "https://tokarskyibogdan.github.io/subly",
  "scripts": {
    "lint-fix": "eslint --ext .tsx,.ts --fix",
    "check:format": "prettier --check ./src",
    "reformat": "prettier --write ./src",
    "check:lint": "eslint ./src",
    "check:types": "tsc",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "classnames": "^2.5.1",
    "cra-template-typescript": "1.2.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.83.1",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "gh-pages": "^6.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
